<div class="main">
  <%= form_for :user, url: users_path do |user| %>  
  <div class="field"> 
    <%= user.label :first_name %>
    <%= user.text_field :first_name %> 
  </div> 
    <div class="field"> 
    <%= user.label :last_name %>
    <%= user.text_field :last_name %> 
  </div> 
    <div class="field"> 
    <%= user.label :email %>
    <%= user.email_field :email %> 
  </div> 
    <div class="field"> 
    <%= user.label :password %>
    <%= user.password_field :password %> 
  </div> 

  <div class="actions"> 
    <%= user.submit "Create" %> 
  </div> 
<% end %>
</div>